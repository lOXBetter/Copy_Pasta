{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "automationAccounts_aa_startstopv2_name": {
      "defaultValue": "aa-startstopv2",
      "type": "String"
    }
  },
  "variables": {},
  "resources": [
    {
      "type": "Microsoft.Automation/automationAccounts/runbooks",
      "apiVersion": "2022-08-08",
      "name": "[concat(parameters('automationAccounts_aa_startstopv2_name'), '/AzureAutomationTutorialWithIdentity')]",
      "location": "centralus",
      "properties": {
        "description": " An example runbook which gets all the ARM resources using the Managed Identity.",
        "logVerbose": false,
        "logProgress": false,
        "logActivityTrace": 0,
        "runbookType": "PowerShell"
      }
    }
  ]
}